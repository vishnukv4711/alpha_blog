<%=form_with(model: @users, local: true) do |f|%>
  <p>
    <%=f.label :username %><br>
    <%=f.text_field :username %>
  </p>
  <p>
    <%=f.label :email %><br>
    <%=f.email_field :email %>
  </p>
  <p>
    <%=f.label :password %><br>
    <%=f.password_field :password %>
  </p>
  <p>
    <%=f.submit %>
  </p>
<%end%>